{
    "version": "16",
        "credential": {
            "accessKey": "{{ minio_access_key }}",
            "secretKey": "{{ minio_secret_key }}"
        },
        "region": "us-east-1",
        "browser": "on",
        "logger": {
            "console": {
                "enable": true
                "filename": "{{ minio_server_logdir }}/minio_console.log",
                "level": "debug"
            },
            "file": {
                "enable": true,
                "filename": "{{ minio_server_logdir }}/minio_file.log",
                "level": "debug"
            }
        }
}
